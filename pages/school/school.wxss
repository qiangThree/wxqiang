.school{
  width:100%;
  height:100%;
  font-size:14px;
  padding: 14rpx;
  box-sizing: border-box;
}
.top,.top-title{
  width:100%;
  padding:2px 0;
}
.top .title{
  font-weight: 700;
}
.pic{
	width:100%;
	height:224px;
	margin:5px 0;
}
.pic image{
	width:100%;
	height: 100%;
}
.list{
	width:100%;
}
.list .title{
	font-weight: 700;
}
.pic2{
	width:100%;
	height:450px;
	margin:5px 0;
}
.pic2 image{
	width:100%;
	height: 100%;
}
.bottom{
	width:100%;
	height:50px;
	display: flex;
	justify-content: center;
	align-items: center;
	color: rgb(63, 63, 63);
	font-size:12px;
}
.bottom image{
	width:20px;
	height:20px;
	margin-right:2px;
}
